<?xml version="1.0" encoding="UTF-8" ?>
<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>1. Register Mother</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/987F4A3A-68AC-4EC4-90E3-7A22A4B75260" uiVersion="1" version="1" name="1. Register Mother">
					<hh_num />
					<mother_name />
					<next_of_kin />
					<age_mother_years />
					<age_of_women />
					<phone_question />
					<phone_number />
					<has_maternal_health_card />
					<previous_pregnancies />
					<num_previous_pregnancies />
					<where_birth_previously />
					<num_living_children />
					<relationship_status />
					<is_pregnant />
					<pregnancy_questions>
						<edd />
						<edd_label />
						<lmp />
						<estimate_edd />
					</pregnancy_questions>
					<not_pregnant_questions>
						<using_fp />
						<counsel_mother_on_fp />
						<high_risk_warning />
					</not_pregnant_questions>
					<number_of_young_children />
					<child_1>
						<name_child_1 />
						<other_name_child_1 />
						<gender_child_1 />
						<dob_child_1 />
					</child_1>
					<child_2>
						<name_child_2 />
						<other_name_child_2 />
						<gender_child_2 />
						<dob_child_2 />
					</child_2>
					<child_3>
						<name_child_3 />
						<other_name_child_3 />
						<gender_child_3 />
						<dob_child_3 />
					</child_3>
					<child_4>
						<name_child_4 />
						<other_name_child_4 />
						<gender_child_4 />
						<dob_child_4 />
					</child_4>
					<not_registered_label />
					<next_visit_immediate />
					<deregister_mother_label />
					<child_1_visit />
					<child_2_visit />
					<child_3_visit />
					<child_4_visit />
					<form_complete />
					<debug_group>
						<debug />
						<debug-3 />
						<debug-2 />
					</debug_group>
					<mother_dob />
					<high_risk_of_pregnancy />
					<mother_state />
					<create_case />
					<delivery_date />
					<conception_date />
					<next_visit />
					<active_child_1 />
					<num_childcare_visits_child_1 />
					<next_visit_child_1 />
					<num_referrals />
					<active_child_2 />
					<active_child_3 />
					<active_child_4 />
					<num_childcare_visits_child_2 />
					<num_childcare_visits_child_3 />
					<num_childcare_visits_child_4 />
					<next_visit_child_2 />
					<next_visit_child_3 />
					<next_visit_child_4 />
					<num_preg_visits />
					<village />
					<parish />
					<subcounty />
					<register_date />
					<cal_edd />
					<lenght_phone />
					<calculate_year />
				</data>
			</instance>
			<instance id="session" src="jr:instance/session"></instance>
			<bind nodeset="/data/hh_num" type="xsd:string" />
			<bind nodeset="/data/mother_name" type="xsd:string" />
			<bind nodeset="/data/next_of_kin" type="xsd:string" />
			<bind nodeset="/data/age_mother_years" type="xsd:int" constraint=". &lt; 2003 and . &gt; 1955" />
			<bind nodeset="/data/age_of_women" />
			<bind nodeset="/data/phone_question" />
			<bind nodeset="/data/phone_number" type="xsd:string" constraint="string-length(/data/phone_number) = 10" relevant="/data/phone_question = 'yes'" />
			<bind nodeset="/data/has_maternal_health_card" />
			<bind nodeset="/data/previous_pregnancies" />
			<bind nodeset="/data/num_previous_pregnancies" type="xsd:int" constraint=". &gt;= 1" relevant="/data/previous_pregnancies = 'yes'" />
			<bind nodeset="/data/where_birth_previously" relevant="/data/previous_pregnancies = 'yes'" />
			<bind nodeset="/data/num_living_children" type="xsd:int" constraint=". &gt;= 0" relevant="/data/previous_pregnancies = 'yes'" />
			<bind nodeset="/data/relationship_status" />
			<bind nodeset="/data/is_pregnant" required="true()" />
			<bind nodeset="/data/pregnancy_questions" relevant="/data/is_pregnant = 'yes'" />
			<bind nodeset="/data/pregnancy_questions/edd" type="xsd:date" constraint="(int(.) - today()) &lt;= 280 and . &gt;= today()" jr:constraintMsg="jr:itext('edd-constraintMsg')" />
			<bind nodeset="/data/pregnancy_questions/edd_label" />
			<bind nodeset="/data/pregnancy_questions/lmp" type="xsd:date" constraint=". &lt; today() and today() - int(.) &lt; 300" jr:constraintMsg="jr:itext('lmp-constraintMsg')" />
			<bind nodeset="/data/pregnancy_questions/estimate_edd" relevant="/data/pregnancy_questions/edd = ''" />
			<bind nodeset="/data/not_pregnant_questions" relevant="/data/is_pregnant != 'yes'" />
			<bind nodeset="/data/not_pregnant_questions/using_fp" required="false()" />
			<bind nodeset="/data/not_pregnant_questions/counsel_mother_on_fp" relevant="/data/not_pregnant_questions/using_fp = 'no'" />
			<bind nodeset="/data/not_pregnant_questions/high_risk_warning" relevant="/data/high_risk_of_pregnancy = 'yes'" />
			<bind nodeset="/data/number_of_young_children" type="xsd:int" constraint=". &gt;= 0 and . &lt;= 4" jr:constraintMsg="jr:itext('number_of_young_children-constraintMsg')" />
			<bind nodeset="/data/child_1" relevant="/data/number_of_young_children &gt;= 1" />
			<bind nodeset="/data/child_1/name_child_1" type="xsd:string" />
			<bind nodeset="/data/child_1/other_name_child_1" type="xsd:string" />
			<bind nodeset="/data/child_1/gender_child_1" />
			<bind nodeset="/data/child_1/dob_child_1" type="xsd:date" constraint=". &lt;= today() and (today() - .) &lt; 730" jr:constraintMsg="jr:itext('dob_child_1-constraintMsg')" />
			<bind nodeset="/data/child_2" relevant="/data/number_of_young_children &gt;= 2" />
			<bind nodeset="/data/child_2/name_child_2" type="xsd:string" required="true()" />
			<bind nodeset="/data/child_2/other_name_child_2" type="xsd:string" />
			<bind nodeset="/data/child_2/gender_child_2" />
			<bind nodeset="/data/child_2/dob_child_2" type="xsd:date" constraint=". &lt;= today() and (today() - .) &lt; 730" jr:constraintMsg="jr:itext('dob_child_2-constraintMsg')" />
			<bind nodeset="/data/child_3" relevant="/data/number_of_young_children &gt;= 3" />
			<bind nodeset="/data/child_3/name_child_3" type="xsd:string" required="true()" />
			<bind nodeset="/data/child_3/other_name_child_3" type="xsd:string" />
			<bind nodeset="/data/child_3/gender_child_3" />
			<bind nodeset="/data/child_3/dob_child_3" type="xsd:date" constraint=". &lt;= today() and (today() - .) &lt; 730" jr:constraintMsg="jr:itext('dob_child_3-constraintMsg')" />
			<bind nodeset="/data/child_4" relevant="/data/number_of_young_children &gt;= 4" />
			<bind nodeset="/data/child_4/name_child_4" type="xsd:string" required="true()" />
			<bind nodeset="/data/child_4/other_name_child_4" type="xsd:string" />
			<bind nodeset="/data/child_4/gender_child_4" />
			<bind nodeset="/data/child_4/dob_child_4" type="xsd:date" constraint=". &lt;= today() and (today() - .) &lt; 730" jr:constraintMsg="jr:itext('dob_child_4-constraintMsg')" />
			<bind nodeset="/data/not_registered_label" relevant="/data/create_case = 'no'" />
			<bind nodeset="/data/next_visit_immediate" relevant="/data/next_visit != '' and /data/create_case = 'yes'" />
			<bind nodeset="/data/deregister_mother_label" relevant="/data/next_visit = '' and /data/create_case = 'yes'" />
			<bind nodeset="/data/child_1_visit" relevant="/data/active_child_1 = 'yes'" />
			<bind nodeset="/data/child_2_visit" relevant="/data/active_child_2 = 'yes'" />
			<bind nodeset="/data/child_3_visit" relevant="/data/active_child_3 = 'yes'" />
			<bind nodeset="/data/child_4_visit" relevant="/data/active_child_4 = 'yes'" />
			<bind nodeset="/data/form_complete" />
			<bind nodeset="/data/debug_group" relevant="1=2" />
			<bind nodeset="/data/debug_group/debug" />
			<bind nodeset="/data/debug_group/debug-3" />
			<bind nodeset="/data/debug_group/debug-2" />
			<bind nodeset="/data/mother_dob" calculate="date(int(today()) - int((/data/age_mother_years*365.25))) " />
			<bind nodeset="/data/high_risk_of_pregnancy" calculate="if((/data/relationship_status = &quot;married&quot; or /data/relationship_status = &quot;relationship&quot;) and /data/not_pregnant_questions/using_fp = 'no', 'yes', 'no')" />
			<bind nodeset="/data/mother_state" calculate="if(/data/is_pregnant = &quot;yes&quot;, &quot;pregnant&quot;, if(/data/number_of_young_children &gt;= 1, &quot;childcare&quot;, &quot;check&quot;))" />
			<bind nodeset="/data/create_case" calculate="if(/data/mother_state = &quot;check&quot;, &quot;no&quot;, &quot;yes&quot;)" />
			<bind nodeset="/data/delivery_date" calculate="if(/data/number_of_young_children = &quot;&quot; or /data/number_of_young_children &lt; 1, &quot;&quot;, date(min(if(/data/child_1/dob_child_1 = &quot;&quot;, today(), /data/child_1/dob_child_1),if(/data/child_2/dob_child_2 = &quot;&quot;, today(), /data/child_2/dob_child_2),if(/data/child_3/dob_child_3 = &quot;&quot;, today(), /data/child_3/dob_child_3),if(/data/child_4/dob_child_4 = &quot;&quot;, today(), /data/child_4/dob_child_4))))" />
			<bind nodeset="/data/conception_date" calculate="date(/data/pregnancy_questions/edd - 280) " />
			<bind nodeset="/data/next_visit" relevant="/data/is_pregnant = 'yes'" calculate="today()" />
			<bind nodeset="/data/active_child_1" calculate="if(/data/number_of_young_children &gt;= 1, &quot;yes&quot;, &quot;no&quot;)" />
			<bind nodeset="/data/num_childcare_visits_child_1" calculate="0" />
			<bind nodeset="/data/next_visit_child_1" calculate="if(/data/child_1/dob_child_1 = &quot;&quot;, &quot;&quot;, if(today() &lt;= date(/data/child_1/dob_child_1 + 42), today(),  date(today())))" />
			<bind nodeset="/data/num_referrals" calculate="0" />
			<bind nodeset="/data/active_child_2" calculate="if(/data/number_of_young_children &gt;= 2, &quot;yes&quot;, &quot;no&quot;)" />
			<bind nodeset="/data/active_child_3" calculate="if(/data/number_of_young_children &gt;= 3, &quot;yes&quot;, &quot;no&quot;)" />
			<bind nodeset="/data/active_child_4" calculate="if(/data/number_of_young_children &gt;= 4, &quot;yes&quot;, &quot;no&quot;)" />
			<bind nodeset="/data/num_childcare_visits_child_2" calculate="0" />
			<bind nodeset="/data/num_childcare_visits_child_3" calculate="0" />
			<bind nodeset="/data/num_childcare_visits_child_4" calculate="0" />
			<bind nodeset="/data/next_visit_child_2" calculate="if(/data/child_2/dob_child_2 = &quot;&quot;, &quot;&quot;, if(today() &lt;= date(/data/child_2/dob_child_2 + 42), today(),  date(today())))" />
			<bind nodeset="/data/next_visit_child_3" calculate="if(/data/child_3/dob_child_3 = &quot;&quot;, &quot;&quot;, if(today() &lt;= date(/data/child_3/dob_child_3 + 42), today(),  date(today())))" />
			<bind nodeset="/data/next_visit_child_4" calculate="if(/data/child_4/dob_child_4 = &quot;&quot;, &quot;&quot;, if(today() &lt;= date(/data/child_4/dob_child_4 + 42), today(),  date(today())))" />
			<bind nodeset="/data/num_preg_visits" calculate="0" />
			<bind nodeset="/data/village" calculate="if(count(instance('session')/session/user/data/village) &gt; 0, instance('session')/session/user/data/village, &quot;Unknown&quot;)" />
			<bind nodeset="/data/parish" calculate="if(count(instance('session')/session/user/data/parish) &gt; 0, instance('session')/session/user/data/parish, &quot;Unknown&quot;)" />
			<bind nodeset="/data/subcounty" calculate="if(count(instance('session')/session/user/data/subcounty) &gt; 0, instance('session')/session/user/data/subcounty , &quot;Unknown&quot;)" />
			<bind nodeset="/data/register_date" calculate="date(today())" />
			<bind nodeset="/data/cal_edd" calculate="date(/data/pregnancy_questions/lmp + 274)" />
			<bind nodeset="/data/lenght_phone" calculate="string-length(/data/phone_number)" />
			<bind nodeset="/data/calculate_year" calculate="2013- int(/data/age_mother_years)" />
			<itext>
				<translation lang="en" default="">
					<text id="hh_num-label">
						<value>Village Name</value>
					</text>
					<text id="mother_name-label">
						<value>Mother Name (please check NRC )</value>
					</text>
					<text id="next_of_kin-label">
						<value>Next of Kin</value>
					</text>
					<text id="age_mother_years-label">
						<value>Mother Year of Birth </value>
						<value form="image">jr://file/commcare/image/data/age_mother_years.png</value>
					</text>
					<text id="age_of_women-label">
						<value>Age of Women (<output value="/data/calculate_year"/>) </value>
					</text>
					<text id="phone_question-label">
						<value>Do you/your relative have phone number?</value>
